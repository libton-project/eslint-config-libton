{
  "name": "eslint-config-libton",
  "version": "0.1.3",
  "description": "ESLint configuration for Libton projects, based on Vite React TypeScript template",
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./vite": "./vite.js",
    "./-/@eslint/js": "./-/@eslint/js/index.js",
    "./-/globals": "./-/globals/index.js",
    "./-/eslint-plugin-react-hooks": "./-/eslint-plugin-react-hooks/index.js",
    "./-/eslint-plugin-react-refresh": "./-/eslint-plugin-react-refresh/index.js",
    "./-/typescript-eslint": "./-/typescript-eslint/index.js"
  },
  "files": [
    "index.js",
    "vite.js",
    "-"
  ],
  "scripts": {
    "gen": "./generate-reexports.sh",
    "format": "pnpx prettier --write '**/*.{js,jsx,ts,tsx,json,yml,css,scss,md}' --single-quote",
    "test": "./test.sh"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "vite",
    "react",
    "typescript"
  ],
  "author": "Libton Project",
  "license": "MIT",
  "peerDependencies": {
    "eslint": "^9.0.0"
  },
  "dependencies": {
    "@eslint/js": "^9.0.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^16.2.0",
    "typescript-eslint": "^8.34.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/libton-project/eslint-config-libton.git"
  },
  "bugs": {
    "url": "https://github.com/libton-project/eslint-config-libton/issues"
  },
  "homepage": "https://github.com/libton-project/eslint-config-libton#readme"
}
